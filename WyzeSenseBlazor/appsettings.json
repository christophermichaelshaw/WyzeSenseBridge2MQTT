{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "WyzeSenseCore.WyzeDongle": "Debug"
    }
  },
  "WyzeSettings": {
    "UsbPath": "/dev/hidraw1"
